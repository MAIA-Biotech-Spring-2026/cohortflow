{
  "name": "cohortflow",
  "version": "0.1.0",
  "private": true,
  "description": "Healthcare program intake management system",
  "repository": {
    "type": "git",
    "url": "https://github.com/MAIA-Biotech-Spring-2026/cohortflow.git"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "db:push": "pnpm --filter @cohortflow/db db:push",
    "db:studio": "pnpm --filter @cohortflow/db db:studio",
    "db:seed": "pnpm --filter @cohortflow/db db:seed",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=18.17.0"
  }
}
